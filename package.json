{
  "name": "egg-typed",
  "version": "0.3.4",
  "description": "typed for egg.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "app/**/*.js",
    "config/**/*.js",
    "lib/**/*.js",
    "**/*.d.ts"
  ],
  "dependencies": {
    "egg": "^2.3.0",
    "power-di": "^1.3.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "cheerio": "^1.0.0-rc.2",
    "egg-bin": "^4.3.7",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.17.0",
    "eslint-config-egg": "^6.0.0",
    "supertest": "^3.0.0",
    "rimraf": "^2.6.2",
    "webstorm-disable-index": "^1.2.0",
    "typescript": "^2.7.1",
    "reflect-metadata": "^0.1.12"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
